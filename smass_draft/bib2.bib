	pages = {22},
	file = {Baes et al. - 2011 - EFFICIENT THREE-DIMENSIONAL NLTE DUST RADIATIVE TR.pdf:/Users/sidneylower/Zotero/storage/8FZM5M7Y/Baes et al. - 2011 - EFFICIENT THREE-DIMENSIONAL NLTE DUST RADIATIVE TR.pdf:application/pdf}
