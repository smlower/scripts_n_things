@article{curtis-lake_modelling_2020,
	title = {Modelling the {M}*-{SFR} relation at high redshift: untangling factors driving biases in the intrinsic scatter measurement},
	shorttitle = {Modelling the {M}*-{SFR} relation at high redshift},
	url = {http://arxiv.org/abs/2001.08560},
	abstract = {We present a method to self-consistently propagate M\$\_\{*\}\$ and SFR (\${\textbackslash}Psi\$) uncertainties onto intercept, slope and intrinsic scatter estimates for a simple model of the main sequence of star forming galaxies where \${\textbackslash}Psi = {\textbackslash}alpha + {\textbackslash}beta\$M\$\_\{*\} + {\textbackslash}mathcal\{N\}(0,{\textbackslash}sigma)\$. From simple idealised models set up with broad-band photometry from NIRCam filters at \$z{\textbackslash}sim5\$, we test the method and compare to methods in the literature. Simplifying the \${\textbackslash}Psi\$ estimate by basing it on dust-corrected MUV can help to reduce the impact of template set degeneracies on slope and intercept estimates, but act to bias the intrinsic scatter estimate. Our test scenarios employ standard star-formation histories used in SED-fitting in the literature. We find that priors in age for constant SFHs, and age and \${\textbackslash}tau\_\{SFR\}\$ for delayed histories, act to corral constraints in the M\$\_\{*\}-{\textbackslash}Psi\$ plane. This means that one might infer better \${\textbackslash}Psi\$ constraints than are actually possible because of the underlying priors. These priors are also incompatible with searching for possible mass-dependence of intrinsic scatter in the main sequence, even for the M\$\_\{*\}\$ estimates being used with independent \${\textbackslash}Psi\$ estimates. \${\textbackslash}Psi\$ estimates obtained from H\${\textbackslash}alpha\$ with dust attenuation correction using H\${\textbackslash}beta\$ are subject to uncertain stellar H\${\textbackslash}beta\$ absorption if SFHs are stochastic. Strong constraints on rest-frame UV slope are therefore required in addition to H\${\textbackslash}alpha\$ and H\${\textbackslash}beta\$ fluxes when investigating an increase in scatter to low stellar masses. These can be obtained with NIRSpec R100 spectra. We show that, for simple exposure time calculations assuming point sources without dust, we should be able to probe to log(m\$\_\{*\}\$/M\$\_\{{\textbackslash}odot\}){\textbackslash}sim8.5\$ with JWST at \$z{\textbackslash}sim5\$.},
	urldate = {2020-01-24},
	journal = {arXiv:2001.08560 [astro-ph]},
	author = {Curtis-Lake, E. and Chevallard, J. and Charlot, S.},
	month = jan,
	year = {2020},
	note = {arXiv: 2001.08560},
	keywords = {Astrophysics - Astrophysics of Galaxies},
	annote = {Comment: Version submitted to MNRAS, comments welcome},
	file = {arXiv Fulltext PDF:/Users/sidneylower/Zotero/storage/AN5LTK9K/Curtis-Lake et al. - 2020 - Modelling the M-SFR relation at high redshift un.pdf:application/pdf;arXiv.org Snapshot:/Users/sidneylower/Zotero/storage/PFM4ZRBY/2001.html:text/html}
