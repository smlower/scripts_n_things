	file = {arXiv\:1904.02180 PDF:/Users/sidneylower/Zotero/storage/X6M2VYY4/Speagle - 2019 - dynesty A Dynamic Nested Sampling Package for Est.pdf:application/pdf;arXiv.org Snapshot:/Users/sidneylower/Zotero/storage/YBBQN5WZ/1904.html:text/html}
}


